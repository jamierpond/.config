{
  "title": "Delete word ctrl + d",
  "rules": [
    {
      "description": "Delete word ctrl + d",
      "manipulators": [
        {
          "from": {
            "key_code": "d",
            "modifiers": {
              "mandatory": ["left_control"],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "right_arrow",
              "modifiers": ["left_alt"]
            },
            {
              "key_code": "left_arrow",
              "modifiers": ["left_alt", "left_shift"]
            },
            {
              "key_code": "delete_or_backspace"
            }
          ],
          "type": "basic"
        }
      ]
    }
  ]
}

